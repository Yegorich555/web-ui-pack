{
  "name": "web-ui-pack",
  "version": "0.0.1",
  "description": "Must-have config for Webpack 4",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "npm run pretest-build && jest --coverage --coverageReporters=text --config test/jest.config.js",
    "test-minify": "node ./test/minification/start.js",
    "pretest-build": "npm run build && webpack --config test/browser/srv/webpack.config.js",
    "coverage": "npm run test -- --coverageReporters=text-lcov | coveralls",
    "example": "./node_modules/.bin/webpack-dev-server --hot --open --config example/webpack.devServer.js",
    "webpack-build-dev": "./node_modules/.bin/webpack --mode development --config example/webpack.dev.js",
    "lint": "npm run lint-code && npm run lint-styles",
    "lint-code": "./node_modules/.bin/eslint --fix --cache ./**/*.{js,jsx,ts,tsx,json}",
    "lint-styles": "./node_modules/.bin/stylelint --fix --cache ./**/*.{css,scss,less}",
    "lint-md": "./node_modules/.bin/markdownlint --fix **/*.md --ignore node_modules",
    "build-styles": "gulp",
    "build": "tsc --listEmittedFiles --incremental && gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yegorich555/web-ui-pack.git"
  },
  "keywords": [
    "html",
    "inputs",
    "forms",
    "elements",
    "material",
    "react",
    "vue",
    "vanilla js",
    "accessibility"
  ],
  "author": "Yegor.Golubchik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yegorich555/web-ui-pack/issues"
  },
  "homepage": "https://github.com/Yegorich555/web-ui-pack#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.4",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/puppeteer": "^2.0.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-classnames-advanced": "^1.0.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.1",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.2.0",
    "jest-puppeteer": "^4.4.0",
    "markdownlint": "^0.19.0",
    "markdownlint-cli": "^0.22.0",
    "node-sass": "^4.13.1",
    "postcss": "^7.0.27",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "puppeteer": "^2.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.23.1",
    "stylelint": "^13.1.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.14.2",
    "terser": "^4.6.4",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-mock-server": "^1.0.2"
  },
  "dependencies": {},
  "files": [
    "lib/*"
  ]
}
