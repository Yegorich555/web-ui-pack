{
  "name": "web-ui-pack",
  "version": "0.0.1",
  "description": "Must-have config for Webpack 4",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "npm run lint",
    "serve": "./node_modules/.bin/webpack-dev-server --hot --open --config webpack.devServer.js",
    "build-prod": "./node_modules/.bin/webpack --mode production --config webpack.prod.js",
    "build-dev": "./node_modules/.bin/webpack --mode development --config webpack.dev.js",
    "lint": "npm run lint-code && npm run lint-styles",
    "lint-code": "./node_modules/.bin/eslint --fix --cache ./**/*.{js,jsx,ts,tsx,json}",
    "lint-styles": "./node_modules/.bin/stylelint --fix --cache ./**/*.{css,scss,less}",
    "lint-md": "./node_modules/.bin/markdownlint --fix **/*.md --ignore node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yegorich555/web-ui-pack.git"
  },
  "keywords": [
    "html",
    "inputs",
    "elements",
    "material",
    "react",
    "vue",
    "vanilla js",
    "accessibility"
  ],
  "author": "Yegor.Golubchik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yegorich555/web-ui-pack/issues"
  },
  "homepage": "https://github.com/Yegorich555/web-ui-pack#readme",
  "devDependencies": {
    "@babel/core": "*",
    "@babel/plugin-proposal-class-properties": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "@babel/preset-typescript": "*",
    "autoprefixer": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-plugin-jsx-classnames-advanced": "*",
    "case-sensitive-paths-webpack-plugin": "*",
    "clean-webpack-plugin": "*",
    "copy-webpack-plugin": "*",
    "css-loader": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-alias": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-json": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "file-loader": "*",
    "friendly-errors-webpack-plugin": "*",
    "html-webpack-plugin": "*",
    "markdownlint": "*",
    "markdownlint-cli": "*",
    "node-sass": "*",
    "optimize-css-assets-webpack-plugin": "*",
    "postcss-loader": "*",
    "postcss-normalize": "*",
    "postcss-scss": "*",
    "prettier": "*",
    "prettier-eslint": "*",
    "sass-loader": "*",
    "style-loader": "^0.23.1",
    "stylelint": "*",
    "stylelint-config-prettier": "*",
    "stylelint-config-recommended-scss": "*",
    "stylelint-no-unsupported-browser-features": "*",
    "stylelint-prettier": "*",
    "stylelint-scss": "*",
    "svg-url-loader": "*",
    "typescript": "*",
    "url-loader": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*",
    "webpack-merge": "*",
    "webpack-mock-server": "*",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {},
  "files": [
    "lib/*"
  ]
}
